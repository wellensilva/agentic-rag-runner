name: agentic-rag
on:
  workflow_dispatch:      # rodar manualmente
jobs:
  run:
    runs-on: ubuntu-latest
    steps:
      - uses: actions/checkout@v4
      - uses: actions/setup-python@v5
        with:
          python-version: '3.11'
      - name: Executar runner
        run: python runner.py --task demo --max_results 5
      - name: Anexar logs
        uses: actions/upload-artifact@v4
        with:
          name: logs
          path: logs/
